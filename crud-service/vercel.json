{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist-crud/crud-service/server.js"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/dist-crud/crud-service/server.js" }
  ]
}
